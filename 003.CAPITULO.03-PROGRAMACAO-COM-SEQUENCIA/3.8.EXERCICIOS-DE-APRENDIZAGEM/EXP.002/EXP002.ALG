Algoritmo "EXP002"

// Elaborar um programa de computador que calcule a área de uma circunferência
// e aprensentar o valor da medida calculada

Var

    A, R : ReaL
    
Inicio

    Escreva("Digite o valor do raio: ")
    Leia(R)
    EscrevaL

    PI <- 3.14159625

      A <- PI * Exp(R, 2)
      

    Escreva("O valor do raio é = ", A)
    EscrevaL
    
Fimalgoritmo